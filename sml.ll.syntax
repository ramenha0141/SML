sml = sml_ sml | ε;
sml_ = define ';';
define = identifier '=' expression;
expression = element '|' expression;
element = element_ element | ε;
element_ = str_s
         | str_d
         | identifier
         | paren
         | brace
         | bracket;
paren = '(' expression ')';
brace = '{' expression '}';
bracket = '[' expression ']';