sml = _sml sml | ε;
_sml = define ';';
define = identifier '=' expression;
expression = element '|' expression;
element = _element element | ε;
_element = string
         | identifier
         | paren
         | brace
         | bracket;
paren = '(' expression ')';
brace = '{' expression '}';
bracket = '[' expression ']';